	"version": "0.0.0",
	"main": "dist/src/index.js",
	"module": "dist/src/index.js",
	"exports": {
		".": "./dist/src/index.js",
		"./lit-element-lion.js": "./dist/src/lit-element-lion.js"
	},
	"scripts": {
		"analyze": "cem analyze --litelement",
		"start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"rollup -c rollup.config.js -w --environment BUILD:development\" \"wds\"",
