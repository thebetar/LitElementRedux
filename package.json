		"@lion/form": "^0.14.0",
		"@lion/form-core": "^0.17.0",
		"@lion/icon": "^0.15.0",
		"@lion/input": "^0.17.0",
		"@lion/textarea": "^0.15.0",
		"@lion/validate-messages": "^0.9.0",
		"@reduxjs/toolkit": "^1.8.1",
		"animejs": "^3.2.1",
		"lit": "^2.0.2",
		"pwa-helpers": "^0.9.1",
		"redux": "^4.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"@custom-elements-manifest/analyzer": "^0.4.17",
