{
	"name": "lit-element-lion",
	"description": "Webcomponent lit-element-lion following open-wc recommendations",
	"license": "MIT",
	"author": "lit-element-lion",
	"version": "0.0.0",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"analyze": "cem analyze --litelement",
		"start": "rimraf dist && concurrently -k -r \"rollup -c rollup.config.js -w\" \"wds\"",
		"build": "rimraf dist && rollup -c rollup.config.js",
		"test": "web-test-runner --watch",
		"test:ci": "web-test-runner --silent",
		"e2e:ci": "concurrently \"npm start\" \"cd server && npm start\" \"wait-on http://localhost:8000 && wait-on http://localhost:3000 && cypress run\""
	},
	"dependencies": {
		"@lion/ajax": "^0.14.0",
		"@lion/button": "^0.17.0",
		"@lion/core": "^0.22.0",
		"@lion/dialog": "^0.15.0",
		"@lion/form": "^0.14.0",
		"@lion/form-core": "^0.17.0",
		"@lion/icon": "^0.15.0",
		"@lion/input": "^0.17.0",
		"@lion/textarea": "^0.15.0",
		"@lion/validate-messages": "^0.9.0",
		"@reduxjs/toolkit": "^1.8.1",
		"@rollup/plugin-typescript": "^8.3.2",
		"animejs": "^3.2.1",
		"cypress": "^9.6.0",
		"deepmerge": "^4.2.2",
		"lit": "^2.0.2",
		"pwa-helpers": "^0.9.1",
		"redux": "^4.2.0",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
